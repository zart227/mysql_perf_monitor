# Файл для хранения секретов. Скопируйте его в .env и заполните своими данными.
# Этот файл не должен попадать в Git.

# SSH
SSH_HOST=your_ssh_host
SSH_PORT=22
SSH_USER=your_ssh_user
SSH_PASSWORD=your_ssh_password
SSH_HOSTKEY_ALG="+ssh-rsa,ssh-dss"
SSH_PUBKEY_TYPES="+ssh-rsa,ssh-dss"

# MySQL
MYSQL_HOST=your_mysql_host
MYSQL_PORT=3306
MYSQL_USER=your_mysql_user
MYSQL_PASSWORD=your_mysql_password
MYSQL_DB=

# Email
EMAIL_ENABLED=True
SMTP_SERVER=smtp.example.com
SMTP_PORT=587
SMTP_USER=your_email@example.com
SMTP_PASSWORD=your_email_password
FROM_ADDR=your_email@example.com
TO_ADDRS=recipient1@example.com,recipient2@example.com
EMAIL_REPORT_TIMES=09:00,23:59

# Monitor
MONITOR_WINDOWS_ENABLED=True

# Настройки мониторинга
DEBUG_MODE=False
HIGH_FREQ_CPU_THRESHOLD=80.0
HIGH_FREQ_MEMORY_THRESHOLD=90.0
HIGH_FREQ_MONITORING_INTERVAL=10
CONTINUOUS_MONITOR_INTERVAL_SECONDS=10
MEMORY_MONITOR_INTERVAL_SECONDS=1800

# Настройки логирования
LOG_TO_FILE=True
LOG_TO_CONSOLE=True
LOG_LEVEL=INFO

# AI-интеграция
OPENAI_API_KEY=your_api_key
OPENAI_API_URL=https://api.openai.com/v1/chat/completions
OPENAI_MODEL=gpt-3.5-turbo
PROXY_URL=127.0.0.1:1080
PROXY_TYPE=socks5h
